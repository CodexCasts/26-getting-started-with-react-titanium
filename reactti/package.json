{
  "name": "reactti",
  "version": "1.0.0",
  "description": "Welcome to your Appcelerator Titanium Mobile Project",
  "main": "Resources/app.js",
  "titaniumifier": {
    "transforms": {
      "babelify": {
        "stage": 0,
        "optional": [
          "runtime"
        ],
        "plugins": [
          "react-titanium/lib/babel-plugin:before"
        ]
      }
    }
  },
  "devDependencies": {
    "babelify": "^6.3.0",
    "titaniumifier": "^1.5.0"
  },
  "dependencies": {
    "babel-runtime": "^5.8.20"
  }
}
